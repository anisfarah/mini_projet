		<link rel="stylesheet" href="https://bootswatch.com/5/cyborg/bootstrap.min.css"/>
		<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">

		

		{% block stylesheets %}
			{#{{ encore_entry_link_tags('app') }}#}
		{% endblock %}

		{% block javascripts %}
			{#{{ encore_entry_script_tags('app') }}#}
		{% endblock %}
<style>
 body {
	 background-color:#1c1e21;
 } 
</style>

	</head>
	<body>
		<nav class="navbar navbar-expand-lg navbar-dark" style="background-color:#1c1e21">
			<div class="container-fluid">
				<a class="navbar-brand" href="{{ path('home_caisse') }}"><img style="height:80px;background-color:#1c1e21" src="https://cdn.discordapp.com/attachments/902620158663733333/918207370134429776/logo.png" ></a>
				<div class="collapse navbar-collapse" id="navbarColor02">
					<ul class="navbar-nav me-auto">
						<li class="nav-item" style="margin-left:200px">
							<button type="button" class="btn btn-light" style="font-size:11px;font-family:consolas">
								{{ date('now')|format_datetime('none', 'short', locale='en')  }}<br>
								{{ "now"|date("d/m/Y") }}
							</button>
						</li>
					</ul>
					<ul class="navbar-nav me-auto">
						<li class="nav-item" style="margin-right:-17px;font-size:25px">
							<a class="nav-link active" href="#">
								{% if app.user != null  %}

									<i class="bi bi-person-fill" style="margin-right:5px">{{ app.user.username }}</i>
								{% else %}

									<i class="bi bi-person-fill" style="margin-right:5px">user</i>

								{% endif %}

							</a>
						</li>
					</ul>
					{% if app.user != null %}
						

						<a href="{{path('app_logout')}}">
							<h3>
								<i class="bi bi-door-open" style="color:#E9CD23; right:50px;top:30px;position:absolute"></i>
							</h3>
						</a>
						
					{% else %}
						<a href="{{path('app_logout')}}">
							<h3>
								<i class="bi bi-door-closed" style="color:#E9CD23; right:50px;top:30px;position:absolute"></i>
							</h3>
						</a>

					{% endif %}

				</div>
				<ul class="navbar-nav me-auto">
					<li class="nav-item" style="margin-right:300px">
						<button type="button" class="btn btn-light" style="font-size:11px;font-family:consolas">
							RÃ©servations
