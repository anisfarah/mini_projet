<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link rel="stylesheet" href="https://bootswatch.com/5/cyborg/bootstrap.min.css"/>
		<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">


		{% block stylesheets %}
			{#{{ encore_entry_link_tags('app') }}#}
		{% endblock %}

		{% block javascripts %}
			{#{{ encore_entry_script_tags('app') }}#}
		{% endblock %}
	</head>
	<body>
		<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
			<div class="container-fluid">
				<a class="navbar-brand" href="{{ path('home_caisse') }}"><img  style="height:70px;opacity:0.6;background-color:black" src="https://comita.halber.space/storage/images/places/logos/eSbOcWi3CcN4I7UFrLl8pKhEMC74DU6dDLGvU9Jz.jpg" ></a>


				<div class="collapse navbar-collapse" id="navbarColor02">
					<ul class="navbar-nav me-auto">
						<li class="nav-item" style="margin-left:500px;font-size:25px">
							<a class="nav-link active" href="#">
								{% if app.user != null  %}
									<i class="bi bi-person-fill" style="margin-right:5px">
										{{ app.user.username }}</i>
								{% else %}
									<i class="bi bi-person-fill" style="margin-right:5px">
										user</i>


								{% endif %}


							</a>
						</li>
					</ul>
						{% if app.user != null %}
						

						<a href="{{path('app_logout')}}">
							<h3>
								<i class="bi bi-door-open" style="color:#E9CD23; right:50px;top:30px;position:absolute"></i>
							</h3>
						</a>
						
					{% else %}
						<a href="{{path('app_logout')}}">
							<h3>
								<i class="bi bi-door-closed" style="color:#E9CD23; right:50px;top:30px;position:absolute"></i>
							</h3>
						</a>

					{% endif %}
				</div>
			</div>
		</nav>
		{% block body %}{% endblock %}
	</body>
</html>
